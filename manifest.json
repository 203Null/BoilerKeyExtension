{
  "manifest_version": 2,
  "name": "BoilerKey Helper",
  "version": "0.1",
  "content_scripts": [
    {
      "matches": [
        "<all_urls>"
      ],
      "js": ["jquery-3.3.1.min.js", "jsOTP.min.js", "base32.min.js", "content.js"]
    }
  ],
  "browser_action": {
    "default_icon": "purdue_logo.png"
  },
  "permissions": [
    "storage"
  ]
}